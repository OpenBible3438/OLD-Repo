<!-- ======================= 편성경 ======================= -->
	<!-- 회원 로그인 -->
	<select id="getMemberLogin" parameterType="map" resultType="map">
		SELECT mem_name, mem_nickname
		  FROM MEMBER
		 WHERE mem_id = #{mem_id}
		   AND mem_pw = #{mem_pw}
	</select>
